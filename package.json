{
  "name": "network-visualisation-dashboard",
  "version": "0.0.0",
  "scripts": {
    "start": "concurrently --names \"ANGULAR,CYTOSCAPE\" --colors \"blue,green\" \"ng serve\" \"npm run start:cytoscape\"",
    "postinstall": "cd servers && npm install",
    "start:angular": "ng serve",
    "start:server": "cd servers && node server.js",
    "start:servers": "concurrently --names \"CYTOSCAPE\" --colors \"green\" \"npm run start:cytoscape\"",
    "build": "ng build",
    "dev": "concurrently --names \"ANGULAR,CYTOSCAPE\" --colors \"blue,green\" \"ng serve --open\" \"npm run start:cytoscape\""
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^19.0.0",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@types/cytoscape": "^3.21.9",
    "@types/d3": "^7.4.3",
    "cytoscape": "^3.32.0",
    "d3": "^7.9.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.3",
    "@angular/cli": "^19.2.3",
    "@angular/compiler-cli": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^9.2.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.2"
  }
}